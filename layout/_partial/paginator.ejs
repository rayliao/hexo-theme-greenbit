<div class="paginator">
    <% if(is_post()) { %>
    <%
    var prev = page.prev ? page.prev.path : false
    var next = page.next ? page.next.path : false 
    %>
    <% if(prev) { %>
    <a href="<%- url_for(prev) %>" class="prev">prev post</a>
    <% } %>
    <% if(next) { %>
    <a href="<%- url_for(next) %>" class="next">next post</a>
    <% } %>
    <% } else { %>
    <%
        var prev = page.prev_link
        var next = page.next_link
    %>
    <% if(page.prev) { %>
    <a href="<%- url_for(prev) %>" class="prev">prev</a>
    <% } %>
    <% if(page.next) { %>
    <a href="<%- url_for(next) %>" class="next">next</a>
    <% } %>
    <% } %>

</div>